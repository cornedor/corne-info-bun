{
  "name": "corne-info-astro",
  "sources": [
    {
      "dir": "rescript",
      "subdirs": true
    },
    {
      "dir": "layouts",
      "subdirs": true
    },
    {
      "dir": "pages",
      "subdirs": true
    },
    {
      "dir": "server",
      "subdirs": true
    },
    {
      "dir": "client",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".js",
  "bs-dependencies": [
    "@rescript/react",
    "@glennsl/rescript-json-combinators",
    "@glennsl/rescript-fetch",
    "@ryyppy/rescript-promise",
    "rescript-webapi",
    "@rescript/core",
    "rescript-nodejs"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "shims": {
      "Jsx": "Jsx"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  }
}
